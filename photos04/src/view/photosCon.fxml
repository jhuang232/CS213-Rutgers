<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<GridPane 
	xmlns="http://javafx.com/javafx/8.0.40" 
	xmlns:fx="http://javafx.com/fxml/1" 
	fx:controller="view.PhotosController">
	
	<padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>

	<fx:include fx:id="tag" source="tag.fxml" />
	<fx:include fx:id="photo" source="photo.fxml" />
	<fx:include fx:id="albumDestination" source="albumDestination.fxml" />
	<fx:include fx:id="album" source="album.fxml" />
	<fx:include fx:id="search" source="search.fxml" />
	<fx:include fx:id="user" source="user.fxml" />
    <fx:include fx:id="admin" source="admin.fxml" />
	<fx:include fx:id="login" source="login.fxml" /> 
	
</GridPane>